{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" },
    { "src": "dist/**", "use": "@vercel/static" }
  ],
  "rewrites": [
    { "source": "/api/:match*", "destination": "/api/index.js" },
    { "source": "/(.*)", "destination": "/dist/$1" }
  ]
}
